{
  "name": "@radr/shadowwire",
  "version": "1.1.16",
  "description": "TypeScript SDK for ShadowWire privacy protocol on Solana",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "wasm/",
    "BROWSER_SETUP.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "solana",
    "privacy",
    "zk-proofs",
    "payments",
    "shadowwire",
    "bulletproofs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Radrdotfun/ShadowWire.git"
  },
  "author": "Radr Labs",
  "license": "MIT",
  "scripts": {
    "build": "tsc && node --input-type=module -e \"import fs from 'fs'; fs.cpSync('wasm', 'dist/wasm', {recursive:true});\"",
    "prepublish": "npm run build",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@solana/web3.js": "^1.95.3",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/bs58": "^4.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
